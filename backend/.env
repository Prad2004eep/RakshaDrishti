TWILIO_VERIFY_SERVICE_SID=

# ============================================
# SERVER CONFIGURATION
# ============================================
PORT=
NODE_ENV=

# ============================================
# OPENAI API (Optional - for AI features)
# ============================================
OPENAI_API_KEY=

